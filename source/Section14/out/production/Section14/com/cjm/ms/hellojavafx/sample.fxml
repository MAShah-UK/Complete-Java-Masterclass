<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="com.cjm.ms.hellojavafx.Controller" xmlns:fx="http://javafx.com/fxml"
          alignment="CENTER" hgap="10" vgap="10" onKeyReleased="#handleKeyReleased">
    <TextField fx:id="nameField" GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.columnSpan="2"/>
    <Button fx:id="helloButton" text="Say Hello" onAction="#onButtonClicked"
            GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <Button fx:id="byeButton" text="Say Bye" onAction="#onButtonClicked"
            GridPane.rowIndex="1" GridPane.columnIndex="1" GridPane.halignment="RIGHT"/>
    <CheckBox fx:id="clearCheckBox" text="Clear the field after saying hello or bye" onAction="#handleChange"
              GridPane.rowIndex="2" GridPane.columnIndex="0" GridPane.columnSpan="2"/>
    <Label fx:id="ourLabel" text="Nothing has happened." GridPane.rowIndex="3" GridPane.columnIndex="0"/>
</GridPane>

